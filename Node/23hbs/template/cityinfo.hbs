<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {{>headerlinks}}
</head>

<body>
    {{>headerfile data="testing"}}
    <div class="container">
        <div class="row">
            <div class="col-lg-4 offset-lg-4 mt-5">
                <div class="card">
                    <div class="card-header"><h3>City info</h3></div>
                    <div class="card-body">
                        <form method="get">
                            <input type="text" name="citydata">
                            <input type="submit" name="search">
                        </form>
                    </div>
                </div>
            </div>
            
            {{{ json searchres }}}
            {{{ searchres.lat }}}
            {{{ searchres.lng }}}
        </div>
    </div>
    <script src="/axios.min.js"></script>
<script>
    console.log(axios);
    axios.get('json')
  .then(function (response) {
    // handle success
    console.log(response);
  })
  .catch(function (error) {
    // handle error
    console.log(error);
  })
  .finally(function () {
    // always executed
  });
</script>
</body>

</html>